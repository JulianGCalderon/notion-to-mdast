{
  "name": "notion-to-mdast",
  "version": "1.0.0",
  "type": "module",
  "description": "Notion's markdown export is buggy and lacks support for many features. This library aims to provide customizable alternative by using the official API to translate pages into a syntax tree, which can then be used to generate markdown or other formats.",
  "scripts": {
    "translate": "npx tsc && node scripts/translate.js",
    "search": "npx tsc && node scripts/search.js"
  },
  "author": "Julian G. Calderon",
  "license": "ISC",
  "dependencies": {
    "@notionhq/client": "^2.2.14",
    "@types/mdast": "^4.0.3",
    "@types/unist": "^3.0.2",
    "dotenv": "^16.3.2",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "remark-directive": "^3.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-stringify": "^11.0.0",
    "ts-node": "^10.9.2",
    "unified": "^11.0.4",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
